<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".alltransactionpage">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/ptrantoolbar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/md_red_900"
            app:title="All Transactions"
            app:titleTextColor="@color/material_drawer_dark_header_selection_text">


        </androidx.appcompat.widget.Toolbar>

        <TextView
            android:id="@+id/pooltransactionstxt"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="TextView" />
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>